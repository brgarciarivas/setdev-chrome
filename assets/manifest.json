{
  "manifest_version": 2,
  "name": "Setdev-chrome",
  "description": "Setdev chrome",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Setdev-chrome"

  },
  "background": {
    "persistent": false,
    "scripts": ["OneSignal.js","background.js"]
  },
  
  "permissions": [
    "gcm",
    "notifications",
    "identity",
    "https://github.com/*",
    "https://www.github.com/*",
    "http://www.github.com/*",
    "tabs",
    "<all_urls>",
    "storage"
  ]
}