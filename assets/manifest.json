{
  "manifest_version": 2,
  "name": "Setdev-chrome",
  "description": "Setdev chrome",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Setdev-chrome"

  },
  "background": {"scripts": ["OneSignal.js", "background.js"]},
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "gcm",
    "notifications",
    "identity",
    "https://github.com/*",
    "https://www.github.com/*",
    "http://www.github.com/*",
    "tabs",
    "<all_urls>",
    "storage"
  ]
}