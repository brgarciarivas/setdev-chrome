{
  "manifest_version": 2,
  "name": "Setdev-chrome",
  "description": "Setdev chrome",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Setdev-chrome"

  },
  "background": {"scripts": ["background.js"]},
  
  "permissions": [
    "tabs","https://wwww.github.com/*","http://ww.github.com/*",
    "storage"
    "videoCapture"
  ]
}